{
  "name": "@flyyer/eslint-config",
  "version": "3.0.2",
  "private": false,
  "description": "ESLint rules for our projects.",
  "scripts": {
    "lint": "eslint --config=.eslintrc.js --ext .js --ext .ts --ext .tsx"
